{
    "branches": [
        {
            "name": "main"
        },
        {
            "name": "dev",
            "channel": "dev",
            "prerelease": true
        }
    ],
    "plugins": [
        "@semantic-release/commit-analyzer",
        [
            "@semantic-release/release-notes-generator",
            {
                "preset": "conventionalcommits"
            }
        ],
        [
            "semantic-release-replace-plugin",
            {
                "replacements": [
                    {
                        "files": [
                            "cmd/root.go"
                        ],
                        "from": "\".*.-- HEAD\"",
                        "to": "\"${nextRelease.version} -- HEAD\"",
                        "results": [
                            {
                                "file": "cmd/root.go",
                                "hasChanged": true,
                                "numMatches": 1,
                                "numReplacements": 1
                            }
                        ],
                        "countMatches": true
                    }
                ]
            }
        ],
        [
            "@semantic-release/git",
            {
                "assets": [
                    "cmd/root.go"
                ]
            }
        ]
    ]
}