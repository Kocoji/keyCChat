{
    "branches": [
        {
            "name": "main"
        },
        {
            "name": "dev",
            "channel": "dev",
            "prerelease": true
        }
    ],
    "plugins": [
        "@semantic-release/commit-analyzer",
        [
            "@semantic-release/release-notes-generator",
            {
                "preset": "conventionalcommits",
                "presetConfig": {
                    "types": [
                        {
                            "type": "feat",
                            "section": "Features"
                        },
                        {
                            "type": "fix",
                            "section": "Bug Fixes"
                        },
                        {
                            "type": "build",
                            "section": "Dependencies Updates",
                            "hidden": false
                        }
                    ]
                }
            }
        ],
        "@semantic-release/changelog",
        [
            "semantic-release-replace-plugin",
            {
                "replacements": [
                    {
                        "files": [
                            "cmd/root.go"
                        ],
                        "from": "__VERSION__ = \".*\"",
                        "to": "__VERSION__ = \"${nextRelease.version}\"",
                        "results": [
                            {
                                "file": "cmd/root.go",
                                "hasChanged": true,
                                "numMatches": 1,
                                "numReplacements": 1
                            }
                        ],
                        "countMatches": true
                    }
                ]
            }
        ],
        [
            "@semantic-release/git",
            {
                "assets": [
                    "cmd/root.go"
                ]
            }
        ]
    ]
}